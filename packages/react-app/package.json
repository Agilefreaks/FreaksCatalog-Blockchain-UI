{
  "name": "@project/react-app",
  "version": "1.0.0",
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@ethersproject/contracts": "^5.0.5",
    "@ethersproject/providers": "^5.0.12",
    "@project/contracts": "^1.0.0",
    "@testing-library/dom": "^6.16.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@walletconnect/web3-provider": "^1.3.1",
    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "apollo-utilities": "^1.3.3",
    "bootstrap": "5.1.0",
    "chalk": "^4.1.0",
    "ethers": "^5.4.4",
    "graphql": "^14.7.0",
    "ipfs-http-client": "^45.0.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-bootstrap": "1.6.1",
    "react-dom": "16.14.0",
    "react-scripts": "3.4.3",
    "styled-components": "^5.2.0",
    "web3modal": "^1.9.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@types/react": "16.9.53",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "1.7.0",
    "sass": "1.36.0",
    "sass-loader": "12.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "ipfs": "yarn build && node scripts/ipfs.js",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npx eslint \"**/*.{js,jsx}\"",
    "lint:css": "npx stylelint \"**/*.{css,scss}\""
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "\\.(css|scss|less)$": "<rootDir>/__mocks__/style-mock.js"
    }
  }
}
